.head-container {
  padding: 0 10rpx 0 30rpx;
}
.head-container .user-list {
  display: inline-block;
}
.head-container .user-list .user-info {
  display: inline-block;
  height: 48rpx;
  line-height: 48rpx;
}
.head-container .user-list .user-info image {
  width: 40rpx;
  height: auto;
  margin-right: 15rpx;
  border-radius: 50%;
  vertical-align: middle;
  animation: jitter 0.5s;
}
.head-container .user-list .user-info .userName {
  display: none;
  font-size: 24rpx;
  color: #909090;
}
.head-container .user-list .user-info.hidden {
  display: none;
}
.head-container .user-list.active .userName {
  display: inline-block;
  margin-right: 15rpx;
}
.head-container .switch-bottom {
  display: inline-block;
  vertical-align: top;
  width: 44rpx;
  height: 44rpx;
  margin-top: 6rpx;
  line-height: 35rpx;
  border-radius: 50%;
  background-color: #fff;
  border: 1px solid #e0e0e0;
  text-align: center;
}
.head-container .switch-bottom .iconfont {
  vertical-align: middle;
  color: #969696;
  font-weight: bold;
}
.head-container .switch-bottom .iconfont.icon-fold {
  margin-left: -2rpx;
}
@keyframes jitter {
  40% {
    scale: 1.15;
  }
  60% {
    scale: 0.95;
  }
  80% {
    scale: 1.1;
  }
  100% {
    scale: 1;
  }
}
